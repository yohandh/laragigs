# Dependency directories (should not be committed to avoid repo bloat)
/vendor
/node_modules

# Environment-specific configurations (sensitive and/or should be unique per environment)
.env
.env.backup
.env.production

# Laravel bootstrap cache (generated files that can be recreated)
/bootstrap/cache/
/bootstrap/cache/packages.php
/bootstrap/cache/services.php

# Compiled and optimized assets (should be generated during deployment)
/public/build
/public/hot
/public/storage

# Sensitive keys and runtime storage (sensitive and/or unique per environment)
/storage/*.key
/storage/debugbar/
/storage/framework/cache/data/*
/storage/framework/sessions/*
/storage/framework/testing/*
/storage/framework/views/*

# Build artifacts (environment-specific and can be regenerated)
/build/

# Composer credentials file (contains sensitive information)
auth.json

# Testing artifacts (should not be committed as they are auto-generated)
/.phpunit.cache
.phpunit.result.cache
/coverage/

# Local development environment files (specific to a developer's machine)
Homestead.yaml
Homestead.json
.vagrant

# IDE configuration (developer-specific settings)
.idea
.fleet
.vscode

# System files (not relevant to the project's code)
.DS_Store
Thumbs.db
ehthumbs.db
*.log
*.sql
*.sqlite

# Logs generated by package managers (transient and developer-specific)
npm-debug.log*
yarn-debug.log*
yarn-error.log*
